{
    "manifestVersion": 1,
    "id": "cdktf",
    "name": "CDKTF Azure DevOps Pipelines Task",
    "version": "0.0.1",
    "publisher": "martenvandijk",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tool for executing CDKTF code in an Azure DevOps pipeline.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}